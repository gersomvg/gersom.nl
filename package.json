{
	"name": "gersom.nl",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"build:icons": "tsx ./src/lib/assets/icon-sprite/generate.ts",
		"sb-pull-components": "storyblok pull-components --space=241655 -p src/lib/storyblok/",
		"sb-generate-types": "storyblok-generate-ts source=./src/lib/storyblok/components.241655.json target=./src/lib/storyblok/storyblok"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.20.4",
		"@types/glob": "8.1.0",
		"autoprefixer": "10.4.14",
		"glob": "10.3.3",
		"node-html-parser": "6.1.5",
		"patch-package": "7.0.2",
		"postcss": "8.4.26",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "3.0.0",
		"prettier-plugin-tailwindcss": "0.4.1",
		"storyblok-generate-ts": "1.15.0",
		"svelte-check": "3.4.3",
		"tailwindcss": "3.3.3",
		"tslib": "2.4.1",
		"tsx": "3.12.7",
		"typescript": "5.1.6",
		"vite": "4.3.6",
		"vitest": "0.32.2"
	},
	"type": "module",
	"dependencies": {
		"storyblok-js-client": "5.12.0",
		"svelte": "4.1.1"
	}
}
